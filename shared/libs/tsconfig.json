{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@angular/*": [
        "node_modules/@angular/*"
      ],
      "event-proxy-lib": [
        "dist/event-proxy-lib"
      ],
      "event-proxy-lib/*": [
        "dist/event-proxy-lib/*"
      ],
      "@protocol-shared/": [
        "../shared"
      ],
      "@protocol-shared/*": [
        "../shared/*"
      ]
    }
  }
}
