{
  "name": "menu-uf",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 3002",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate-translation": "ng xi18n --output-path ./locales --format=xmb",
    "build-config": "tsc --project tsconfig.conf-builder.json",
    "ng_build_prod": "ng build --localize --prod --outputHashing=none --output-path prod/",
    "ng_serve_prod": "serve -C -l tcp://0.0.0.0:8082 prod/",
    "ng_start": "npm run ng_build_prod && npm run npm_serve_prod",

    "ng_serve_dev": "serve --cors -l tcp://0.0.0.0:3002 dist/",
    "ng_build_dev": "ng build --localize --commonChunk=false --outputHashing=none --vendorChunk=false",
    "ng_watch": "ng build --localize --commonChunk=false --outputHashing=none --vendorChunk=false --watch",
    "ng_con": "npm run build-config && concurrently --kill-others -n \"ANGULAR,SERVER\" \"npm run ng_watch\"  \"npm run ng_serve_dev\" "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.7",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.0.7",
    "@angular/compiler": "^9.0.7",
    "@angular/core": "^9.0.7",
    "@angular/elements": "^9.1.12",
    "@angular/forms": "^9.0.7",
    "@angular/localize": "^9.1.12",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.0.7",
    "@angular/platform-browser-dynamic": "^9.0.7",
    "@angular/router": "^9.0.7",
    "document-register-element": "^1.14.5",
    "rxjs": "^6.5.5",
    "tslib": "^1.13.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.2.0",
    "@angular-devkit/build-angular": "^0.900.7",
    "@angular/cli": "^9.0.7",
    "@angular/compiler-cli": "^9.0.7",
    "@angular/language-service": "^9.0.7",
    "@types/jasmine": "^3.5.12",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.54",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "codelyzer": "^5.2.2",
    "concurrently": "^5.3.0",
    "eslint": "^7.7.0",
    "eslint-plugin-jsdoc": "^25.4.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "^5.4.4",
    "serve": "^11.3.2",
    "ts-node": "~8.3.0",
    "typescript": "~3.7.5"
  }
}
