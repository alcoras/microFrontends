{
  "name": "template",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port $dev_port$",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ng_build_prod": "ng build --prod --outputHashing=none --output-path prod/$project_name$",
    "ng_serve_prod": "serve -l tcp://0.0.0.0:$prod_port$ prod/$project_name$",
    "ng_start_prod": "npm run ng_build_prod && npm run npm_serve_prod",
    "ng_build_dev": "ng build --commonChunk=false --outputHashing=none --vendorChunk=false",
    "ng_serve_dev": "serve -l tcp://0.0.0.0:$dev_port$ dist/$project_name$",
    "ng_watch": "ng build --commonChunk=false --outputHashing=none --vendorChunk=false --watch",
    "ng_con": "concurrently --kill-others -n \"ANGULAR,SERVER\" \"npm run ng_watch\"  \"npm run ng_serve_dev\" "
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular/animations": "~10.0.11",
    "@angular/common": "~10.0.11",
    "@angular/compiler": "~10.0.11",
    "@angular/core": "~10.0.11",
    "@angular/elements": "^10.1.1",
    "@angular/forms": "~10.0.11",
    "@angular/platform-browser": "~10.0.11",
    "@angular/platform-browser-dynamic": "~10.0.11",
    "@angular/router": "~10.0.11",
    "concurrently": "^5.3.0",
    "event-proxy-lib": "^2.1.0",
    "rxjs": "~6.5.5",
    "serve": "^11.3.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.7",
    "@angular/cli": "~10.0.7",
    "@angular/compiler-cli": "~10.0.11",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "codelyzer": "^6.0.0",
    "eslint": "^7.9.0",
    "eslint-plugin-jsdoc": "^30.5.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~3.9.5"
  }
}
